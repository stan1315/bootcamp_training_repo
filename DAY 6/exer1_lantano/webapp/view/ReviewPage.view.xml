<mvc:View controllerName="com.training.exer1lantano.controller.ReviewPage"
  xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Page id="idReviewPage" title="{i18n>reviewTitle}" floatingFooter="true">
    <content>
      <core:Fragment fragmentName="com.training.exer1lantano.fragment.ShoppingCart" type="XML" />
    </content>
    <footer>
      <OverflowToolbar id="idOFTFooterReview">
        <ToolbarSpacer id="idTSpacerFooterReview"/>
        <Button id="idBtnBack" text="{i18n>backBtn}" tooltip="{i18n>backTooltip}" press="onPressBack"/>
      </OverflowToolbar>
    </footer>
  </Page>
</mvc:View>